<% @users.each_with_index do |user, rank| %>
  <%= user[:name] %> completed <%= user[:labs_completed] %> labs, <%= rank < 6 ? "woo" : "boo" %><br>
<% end %>

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

<%= javascript_tag do -%>
  google.charts.load('current', {'packages':['bar']});
  google.charts.setOnLoadCallback(drawChart);

  const dataTable = [['Boolean Icee', 'Labs Completed']]

  for (const user of JSON.parse('<%= @users_json.html_safe -%>')) {
    dataTable.push([user.name, user.labs_completed])
  }

  function drawChart() {
    var chartData = google.visualization.arrayToDataTable(dataTable);

    var options = {
      chart: {
        title: 'Labs Completed',
        subtitle: 'cookies are tasty',
      }
    };

    var chart = new google.charts.Bar(document.getElementById('chart_div'));

    chart.draw(chartData, google.charts.Bar.convertOptions(options));
  }
<% end -%>




<div id="chart_div" width="800" height="600"></div>